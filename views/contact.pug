extends base.pug

block scripts
    script(src='/static/index.js')

block style
    style
      include ../static/style.css
      include ../static/stylecon.css


block content
    div.con
        h1 Contact Us
        form(action="/contact",method="post",class="myform")
            input(type="text",class="myinput",name="name",placeholder="Enter ur Name")
            input(type="age",class="myinput",name="age",placeholder="Enter ur age")
            input(type="phone",class="myinput",name="phone",placeholder="Enter ur phonenumber")
            input(type="email",class="myinput",name="email",placeholder="Enter ur email")
            input(type="text",class="myinput",name="address",placeholder="Enter ur address")
            input(type="text",class="myinput",name="desc",placeholder="Enter ur issue")
            button(class="btn")
                | Submit